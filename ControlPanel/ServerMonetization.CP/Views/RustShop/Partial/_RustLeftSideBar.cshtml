            </a>
            <a class="list-group-item" asp-area="" asp-controller="RustShop" asp-action="SalesHistory" asp-route-shopId="@Model.Id">
                Sales history
            </a>
            <a class="list-group-item" asp-area="" asp-controller="RustShop" asp-action="ServersManager" asp-route-shopId="@Model.Id">
