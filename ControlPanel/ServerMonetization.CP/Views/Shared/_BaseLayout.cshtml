<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewData["Title"]</title>
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=no">
    <link rel="icon" type="image/png" href="~/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="~/favicon-16x16.png" sizes="16x16">
    <link rel="manifest" href="~/manifest.json">
    <meta name="theme-color" content="#1b1b1b">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,400italic,500,700">

    <link rel="stylesheet" href="~/css/vendor.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/elephant.min.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/application.min.css" asp-append-version="true">


    <link rel="stylesheet" href="~/css/custom/Override/override-elephant.css" asp-append-version="true">
    <link rel="stylesheet" href="~/css/custom/Override/override-application.css" asp-append-version="true">


    @RenderSection("Styles", false)
</head>
<body>
    @RenderBody()
    @RenderSection("Scripts", false)
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-157496649-1"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'UA-157496649-1');
    </script>

</body>
</html>